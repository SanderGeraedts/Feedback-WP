{
  "name": "silverbee/silverbee-starter",
  "description": "The starter theme for Silverbee V.O.F.",
  "authors": [
    {
      "name": "Silverbee V.O.F.",
      "email": "sander@silverbee.nl",
      "homepage": "http://www.silverbee.nl/"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "vendor-dir": "wp-content/vendor"
  },
  "require": {
    "johnpbloch/wordpress": ">=4.9.8",
    "wpackagist-plugin/wp-migrate-db": ">=1.0.6",
    "wpackagist-plugin/contact-form-7": ">=5.0.5",
    "wpackagist-plugin/mailchimp-for-wp": ">=4.2.5",
    "wpackagist-plugin/query-monitor": ">=3.1.1",
    "wpackagist-plugin/tiny-compress-images": ">=3.1.0",
    "wpackagist-plugin/gdpr": ">=2.1.0",
    "wpackagist-plugin/really-simple-ssl": ">=3.1.2",
    "wpackagist-plugin/wordpress-seo": ">=9.2"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  }
}